{
  "name": "jsonbro",
  "displayName": "JSONBro",
  "description": "A simple VS Code extension for working with JSON.",
  "version": "0.2.3",
  "publisher": "LucianDCrainic",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jsonbro.formatJson",
        "title": "JSONBro: Format JSON"
      },
      {
        "command": "jsonbro.diffJson",
        "title": "JSONBro: Diff JSON"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run clean && npm run compile",
    "compile": "webpack --mode production",
    "compile:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "package": "npm run vscode:prepublish && vsce package",
    "clean": "rimraf out",
    "test": "jest",
    "lint": "echo 'Linting passed - no linter configured yet'",
    "prepackage": "npm test"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.81.0",
    "jest": "^30.2.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.4.4",
    "typescript": "^5.0.2",
    "vsce": "^2.15.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  }
}
